# GitHub App credentials (from github.com/settings/apps)
GITHUB_APP_ID=
GITHUB_APP_PRIVATE_KEY="-----BEGIN RSA PRIVATE KEY-----\n...\n-----END RSA PRIVATE KEY-----"
WEBHOOK_SECRET=

# OpenRouter API key (from openrouter.ai/keys)
OPENROUTER_API_KEY=
OPENROUTER_MODEL=google/gemini-2.0-flash-001

# Comma-separated GitHub org IDs for closed beta (empty = allow all)
ALLOWED_ORG_IDS=

# Server port
PORT=3000
